{
    "api": {
        "services": [
            "HandlerService",
            "StatsService"
        ],
        "tag": "api"
    },
    "inbounds": [
        {
            "port": 2323,
            "protocol": "dokodemo-door",
            "settings": {
                "address": "127.0.0.1"
            },
            "tag": "api"
        },
        {{- range $index, $inbound := .Inbounds }}
        {
            "port": "{{ $inbound.InPort }}",
            "protocol": "{{ $inbound.Proxy }}",
            "streamSettings": {
                "network": "{{ $inbound.Transport }}",
                "security": "{{ $inbound.Security }}"
                {{- if eq $inbound.Security "tls" }},
                "tlsSettings": {
                    "allowInsecure": true,
                    "certificates": [
                        {
                            "certificateFile": "{{ $inbound.TLSCertPath }}",
                            "keyFile": "{{ $inbound.TLSKeyPath }}"
                        }
                    ]
                }
                {{- end }}
            },
            "tag": "{{ .Tag }}"
        }
        {{- if ne (sum $index 1) (len $.Inbounds) }},{{- end }}
        {{- end }}
    ],
    "log": {
        "access": "none",
        "error": "none",
        "loglevel": "none"
    },
    "outbounds": [
        {
            "protocol": "freedom"
        }
    ],
    "policy": {
        "levels": {
            "0": {
                "statsUserDownlink": true,
                "statsUserUplink": true
            }
        }
    },
    "routing": {
        "rules": [
            {
                "inboundTag": [
                    "api"
                ],
                "outboundTag": "api",
                "type": "field"
            }
        ]
    },
    "stats": {},
    "transport": {
        "dsSettings": {},
        "grpcSettings": {},
        "gunSettings": {},
        "httpSettings": {},
        "kcpSettings": {},
        "quicSettings": {
            "security": "chacha20-poly1305"
        },
        "tcpSettings": {},
        "wsSettings": {}
    }
}
